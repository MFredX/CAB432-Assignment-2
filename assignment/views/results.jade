extends layout

block content
    .ui.container
        .ui.basic.center.aligned.segment
            div(style="margin-bottom:40px;")
                h1 Results for: 

                each hashtag in hashtags
                    .ui.image.label
                        | # #{hashtag}
            each tweet in tweetData
                div(class='infoBlock')
                    p Tweet: #{tweet[0]}
                    p Score: #{tweet[1]}